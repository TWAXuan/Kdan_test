<h1>書店</h1>

<table class= "table">
  <thead>
    <tr>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.name %></td>
        <td><%= link_to 'Edit', edit_store_path(store) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>